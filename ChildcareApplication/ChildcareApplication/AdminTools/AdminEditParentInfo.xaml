        <Label x:Name="lbl_UserID" Content="ID Number:" HorizontalAlignment="Left" Margin="12,195,0,0" VerticalAlignment="Top" Width="83"/>
        <TextBox x:Name="txt_IDNumber" HorizontalAlignment="Left" Height="23" Margin="81,198,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="89" IsEnabled="False"/>
