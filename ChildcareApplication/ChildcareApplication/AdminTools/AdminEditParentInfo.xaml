        <Button x:Name="btn_Delete" Content="Delete" HorizontalAlignment="Left" Margin="43,376,0,0" VerticalAlignment="Top" Width="92" Height="38" Click="btn_Delete_Click"/>
        <Label x:Name="lbl_Email" Content="Email:" HorizontalAlignment="Left" Margin="247,148,0,0" VerticalAlignment="Top" Width="47"/>
        <TextBox x:Name="txt_Email" HorizontalAlignment="Left" Height="23" Margin="308,150,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="161"/>
