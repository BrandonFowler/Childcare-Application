<Window x:Class="AdminTools.ParentIDEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="win_ParentLogin" Height="252.621" Width="378.238"
        AllowsTransparency="True"
        WindowStyle="None"
        Padding="0"
        WindowStartupLocation="CenterScreen" Background="#FFE4E8EE">
    <Border BorderBrush="#FFE4ECEE" BorderThickness="2" Width="378.238" Height="252.621" ClipToBounds="True">
        <Grid>
            <TextBox x:Name="txt_IDEntry" HorizontalAlignment="Left" Height="23" Margin="125,70,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120" Background="#FFE6EAE0"/>
            <Label x:Name="lbl_ID" Content="Parent ID" HorizontalAlignment="Left" Margin="125,44,0,0" VerticalAlignment="Top"/>
            <Button Style="{DynamicResource GlassButton}" x:Name="btn_Submit" Content="Submit" HorizontalAlignment="Left" Margin="125,139,0,0" VerticalAlignment="Top" Width="117" Height="43" Click="btn_Submit_Click"/>

        </Grid>
    </Border>
</Window>
