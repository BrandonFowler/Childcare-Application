        <Button x:Name="btn_Enter" Content="Enter" HorizontalAlignment="Left" Margin="192,155,0,0" VerticalAlignment="Top" Width="124" Height="61" Click="btn_Enter_Click"/>
        <Label Content="Please enter the Guardian ID of the person you with to link or delink your child." HorizontalAlignment="Left" Margin="33,10,0,0" VerticalAlignment="Top" Width="427"/>
